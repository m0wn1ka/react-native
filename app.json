{
  "expo": {
    "name": "radha",
    "slug": "radha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/radha.jpg",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/radha.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#eeeee"
    },
    "main": "expo-router/entry",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
